<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="data.com.lamegaboxv2.boxes.ZelkysFragment"
    android:background="@color/limeMaterial">

    <TextView
        android:id="@+id/titleZelkys"
        android:text="@string/zelkys"
        android:layout_width="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="25sp"
        android:textColor="@color/white"/>



    <ImageButton
        android:id="@+id/zelkysOne"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/titleZelkys"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_zelkys"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/zelkysTwo"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/titleZelkys"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_zelkys"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/zelkysThree"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/zelkysTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_zelkys"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/zelkysFour"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/zelkysTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_zelkys"
        android:layout_centerHorizontal="true"
        />



    <ImageButton
        android:id="@+id/zelkysFive"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/zelkysThree"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_zelkys"
        android:layout_centerHorizontal="true"
        />


</RelativeLayout>
