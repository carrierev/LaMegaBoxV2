<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="data.com.lamegaboxv2.boxes.TlauFragment"
    android:background="@color/purpleMaterial">

    <TextView
        android:id="@+id/titleTlow"
        android:text="@string/tlow"
        android:layout_width="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="25sp"
        android:textColor="@color/white"/>
    <ImageButton
        android:id="@+id/tlowOne"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/titleTlow"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_1tlau"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/tlowTwo"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/titleTlow"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_1tlau"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/tlowThree"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tlowTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_1tlau"
        android:layout_centerHorizontal="true"
        />

    <ImageButton
        android:id="@+id/tlowFour"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/tlowTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_1tlau"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/tlowFive"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/tlowThree"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_1tlau"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/tlowSix"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/tlowFour"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_1tlau"
        android:layout_centerHorizontal="true"
        />


</RelativeLayout>
