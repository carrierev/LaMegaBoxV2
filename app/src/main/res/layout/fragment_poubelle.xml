<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="data.com.lamegaboxv2.boxes.PoubelleFragment"
    android:background="@color/grey900Material">

    <TextView
        android:id="@+id/titlePoubelle"
        android:text="@string/poubelle"
        android:layout_width="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="25sp"
        android:textColor="@color/white"/>



    <ImageButton
        android:id="@+id/poubelleOne"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/titlePoubelle"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_launcher"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/poubelleTwo"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/titlePoubelle"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_launcher"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/poubelleThree"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/poubelleTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_launcher"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/poubelleFour"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/poubelleTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_launcher"
        android:layout_centerHorizontal="true"
        />



    <ImageButton
        android:id="@+id/poubelleFive"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/poubelleThree"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/ic_launcher"
        android:layout_centerHorizontal="true"
        />


</RelativeLayout>
