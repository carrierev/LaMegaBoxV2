<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="data.com.lamegaboxv2.boxes.CjpFragment"
    android:background="@color/blueA200Material">

    <TextView
        android:id="@+id/titleCjp"
        android:text="@string/cjp"
        android:layout_width="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_height="wrap_content"
        android:layout_marginTop="15dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="25sp"
        android:textColor="@color/white"/>
    <ImageButton
        android:id="@+id/cjpOne"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/titleCjp"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/cjpTwo"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/titleCjp"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/cjpThree"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/cjpTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />

    <ImageButton
        android:id="@+id/cjpFour"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/cjpTwo"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/cjpFive"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cjpThree"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />

    <ImageButton
        android:id="@+id/cjpSix"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/cjpFour"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />

    <ImageButton
        android:id="@+id/cjpSeven"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/cjpSix"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />

    <ImageButton
        android:id="@+id/cjpEight"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_below="@+id/cjpSix"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />


    <ImageButton
        android:id="@+id/cjpNine"
        style="?android:borderlessButtonStyle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/cjpSeven"
        android:contentDescription="@string/soundButton"
        android:src="@drawable/balignon"
        android:layout_centerHorizontal="true"
        />


</RelativeLayout>
